{
    "name": "bygi/woocommerce-custom-checkout-fields",
    "description": "WordPress плагин для WooCommerce, который добавляет кастомные поля в процесс оформления заказа и интегрирует их с аналитикой WooCommerce",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Mikalai Kazak",
            "email": "mikalai.kazak@example.com",
            "homepage": "https://example.com"
        }
    ],
    "keywords": [
        "wordpress",
        "woocommerce",
        "checkout",
        "custom-fields",
        "analytics",
        "plugin"
    ],
    "homepage": "https://github.com/bygi/woocommerce-custom-checkout-fields",
    "support": {
        "issues": "https://github.com/bygi/woocommerce-custom-checkout-fields/issues",
        "source": "https://github.com/bygi/woocommerce-custom-checkout-fields"
    },
    "require": {
        "php": ">=7.4",
        "composer/installers": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.0",
        "phpcompatibility/php-compatibility": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BYGI\\WooCommerce\\CustomCheckoutFields\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BYGI\\WooCommerce\\CustomCheckoutFields\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress src/",
        "phpcbf": "phpcbf --standard=WordPress src/"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "installer-name": "bygi-custom-checkout-fields",
        "wordpress": {
            "plugin": "bygi-custom-checkout-fields.php"
        }
    }
} 